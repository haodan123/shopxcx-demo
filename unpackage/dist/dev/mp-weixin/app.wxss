/*每个页面公共css */
.red {
  color: red;
}
/* // 卡片 */
.card-box {
  background-color: #fff;
  padding: 10px;
  border-radius: 10px;
}
.card-box .title {
  display: flex;
  padding-left: 5px;
  margin-bottom: 10px;
  justify-content: space-between;
}
.card-box .title .title-right {
  display: flex;
  align-items: center;
  gap: 5px;
}
.card-box .notbm {
  margin-bottom: 0px;
}
.contact {
  padding: 0;
  margin: 0;
  border: none;
  background-color: transparent;
  line-height: inherit;
}
.contact::after {
  border: none;
}
.address-box {
  display: flex;
  flex-direction: column;
  gap: 10px;
}
.address-box .address-item {
  display: flex;
  align-items: center;
  gap: 10px;
}
.heng {
  text-decoration: line-through;
  color: #ccc;
}
